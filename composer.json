{
    "name": "paperscissors/tax-calculator",
    "description": "Simple Tax Calculator for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Tax-Calculator",
        "Laravel Tax Calculator"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8|^6.0|^7.0",
        "illuminate/database": "^5.8|^6.0|^7.0",
        "moneyphp/money": "^3.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\TaxCalculator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\TaxCalculator\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\TaxCalculator\\TaxCalculatorServiceProvider"
            ],
            "aliases": {
                "TaxCalculator": "Artisanry\\TaxCalculator\\Facades\\TaxCalculator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
